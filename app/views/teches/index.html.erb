<p id="notice"><%= notice %></p>

<h1>Teches</h1>

<table>
  <thead>
    <tr>
      <th>Question</th>
      <th>Option1</th>
      <th>Option2</th>
      <th>Option3</th>
      <th>Option4</th>
      <th>Ans</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teches.each do |tech| %>
      <tr>
        <td><%= tech.question %></td>
        <td><%= tech.option1 %></td>
        <td><%= tech.option2 %></td>
        <td><%= tech.option3 %></td>
        <td><%= tech.option4 %></td>
        <td><%= tech.ans %></td>
        <td><%= link_to 'Show', tech %></td>
        <td><%= link_to 'Edit', edit_tech_path(tech) %></td>
        <td><%= link_to 'Destroy', tech, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tech', new_tech_path %>
